[ignore]
.*vendor/.*

[include]

[libs]
client/interfaces

[options]
module.name_mapper='^admin/\(.*\)$' -> '<PROJECT_ROOT>/client/admin/\1'
module.name_mapper='^app/\(.*\)$' -> '<PROJECT_ROOT>/client/app/\1'
module.name_mapper.extension='css' -> 'CSSModule'
module.name_mapper.extension='scss' -> 'CSSModule'
module.file_ext=.js
module.file_ext=.css
module.file_ext=.scss

[version]
^0.82.0
